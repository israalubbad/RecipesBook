<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="RoundedCornerImage">
        <item name="cornerFamily">rounded</item>
        <item name="cornerSize">16dp</item>
    </style>

    <style name="CustomTabText" parent="TextAppearance.Design.Tab">
        <item name="android:textSize">14sp</item>
        <item name="android:textColor">#000000</item>
        <item name="android:textStyle">bold</item>
    </style>


    <style name="MaterialSearchBarDark" parent="Widget.MaterialComponents.TextInputLayout.OutlinedBox">
        <item name="android:textColor">#FFFFFF</item>
        <item name="android:textColorHint">#BBBBBB</item>
        <item name="android:backgroundTint">@color/gray_light</item>
        <item name="mt_navIconTint">@color/white</item>
        <item name="mt_menuIconTint">@color/white</item>
        <item name="mt_hintColor">#BBBBBB</item>
    </style>



</resources>